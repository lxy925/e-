.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}
button {
    background: initial;
}
button:focus {
    outline: 0;
}
button::after {
    border: none;
}
page {
    background: linear-gradient(to bottom, #1cd6c7, #99efe9,rgb(239, 239, 239),rgb(239, 239, 239),rgb(239, 239, 239),rgb(239, 239, 239));
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    min-height: 100vh;
}

